        xmlns:local="clr-namespace:PLCNET5电容塔"
        mc:Ignorable="d"
        Title="ProcessWindow" Height="500" Width="1000">
    <Grid ShowGridLines="True">

        <Grid.RowDefinitions>
            <RowDefinition Height="5*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
