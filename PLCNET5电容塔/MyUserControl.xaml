<UserControl x:Class="PLCNET5电容塔.MyUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PLCNET5电容塔"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid ShowGridLines="True">
        <Grid.RowDefinitions >
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>


        <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="0" >
            <TextBlock Text="AQ1-1" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10 0 5 0" x:Name="q1_1"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_1Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Right" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="0">
            <TextBlock Text="AQ1-2" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q1_2"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_2Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="0">
            <TextBlock Text="AQ1-3" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q1_3"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_3Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.Column="0">
            <TextBlock Text="AQ1-4" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q1_4"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_4Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="4" Grid.Column="0">
            <TextBlock Text="AQ1-5" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q1_5"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_5Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="5" Grid.Column="0">
            <TextBlock Text="AQ1-6" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q1_6"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q1_6Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>


        <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="1">
            <TextBlock Text="BQ1-1" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0"  x:Name="q2_1"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_1Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="1">
            <TextBlock Text="BQ1-2" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q2_2"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_2Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="1">
            <TextBlock Text="BQ1-3" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q2_3"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_3Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="3" Grid.Column="1">
            <TextBlock Text="BQ1-4" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q2_4"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_4Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="4" Grid.Column="1">
            <TextBlock Text="BQ1-5" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q2_5"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_5Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="5" Grid.Column="1">
            <TextBlock Text="BQ1-6" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q2_6"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q2_6Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>


        <StackPanel Orientation="Horizontal"  Grid.Row="0" Grid.Column="2">
            <TextBlock Text="CQ1-1" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_1"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_1Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="2">
            <TextBlock Text="CQ1-2" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_2"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_2Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="2" Grid.Column="2">
            <TextBlock Text="CQ1-3" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_3"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_3Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="3" Grid.Column="2">
            <TextBlock Text="CQ1-4" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_4"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_4Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal"  Grid.Row="4" Grid.Column="2">
            <TextBlock Text="CQ1-5" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_5"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_5Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

        <StackPanel Orientation="Horizontal" Grid.Row="5" Grid.Column="2">
            <TextBlock Text="CQ1-6" FontSize="{Binding MyFS}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10 0 5 0" x:Name="q3_6"></TextBlock>
            <Image Width="{Binding Myw}" Height="{Binding Myh}" Source="{Binding Q3_5Image}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" ></Image>
        </StackPanel>

    </Grid>

</UserControl>
