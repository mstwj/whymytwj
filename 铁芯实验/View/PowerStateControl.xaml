<UserControl x:Class="铁芯实验.View.PowerStateControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:铁芯实验.View"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200">
    
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>

            <StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                <TextBlock Text="电源状态" Margin="0 0 10 0"></TextBlock>
                <TextBlock Text="{Binding Powerstate}"></TextBlock>
            </StackPanel>

            <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                <TextBlock Text="输出档位" Margin="0 0 10 0"></TextBlock>
                <TextBlock Text="{Binding Powergaodi}"></TextBlock>
            </StackPanel>

        </Grid>
    
</UserControl>
