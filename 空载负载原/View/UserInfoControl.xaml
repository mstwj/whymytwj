<UserControl x:Class="空载_负载.View.UserInfoControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:空载_负载.View"
             mc:Ignorable="d" 
             >
    
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <!--
        <StackPanel Grid.Row="0" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="出厂编号:" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding ProductNumber}" FontSize="16"></TextBlock>
        </StackPanel>
        -->

        <StackPanel Grid.Row="0" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="出厂编号:" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding ProductNumber}" FontSize="16"></TextBlock>
        </StackPanel>


        <StackPanel Grid.Row="1" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="规格型号:" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding ProductType}" FontSize="16"></TextBlock>
        </StackPanel>

        <StackPanel Grid.Row="2" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="产品图号:" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding ProductTuhao}" FontSize="16"></TextBlock>
        </StackPanel>
        <StackPanel Grid.Row="3" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="高压侧电压(kV):" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding Highpressure }" FontSize="16"></TextBlock>
        </StackPanel>
        <StackPanel Grid.Row="4" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="高压侧电流(A):" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding Highcurrent }" FontSize="16"></TextBlock>
        </StackPanel>
        <StackPanel Grid.Row="5" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="低压侧电压(kV):" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding Lowpressure }" FontSize="16"></TextBlock>
        </StackPanel>

        <StackPanel Grid.Row="6" Orientation="Horizontal" VerticalAlignment="Center"  HorizontalAlignment="Left">
            <TextBlock  Text="低压侧电流(A):" FontSize="16" Margin="0 00 10 0"></TextBlock>
            <TextBlock Text="{Binding Lowcurrent  }" FontSize="16"></TextBlock>
        </StackPanel>

    </Grid>
</UserControl>
